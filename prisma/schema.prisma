// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Institution { //model = defines a database table
  id         Int          @id @default(autoincrement()) //@id = specify the primary key
  name       String
  region     String
  country    String
  createdAt  DateTime     @default(now()) //these last two do not include in total variables/attributes
  updatedAt  DateTime     @updatedAt
}
